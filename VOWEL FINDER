#include <stdio.h>

int main()
{
    char ch;
    printf("ENTER YOUR CHARACTER:-");
    scanf("%c",&ch);
    switch(ch)
    {
    case 'a':
    case 'e':
    case 'i':
    case 'o':
    case 'u':
    case 'A':
    case 'E':
    case 'I':
    case 'O':
    case 'U':
    printf("ENTERED CHAR IS VOWEL");
    break;
    default:printf("ENTERED CHAR IS NOT VOWEL");

    }    
}
