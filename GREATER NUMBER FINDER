#include <stdio.h>
int main()
{
    int a,b,c;
    printf("enter first number=");
    scanf("%d",&a);
    printf("enter second number=");
    scanf("%d",&b);
    printf("enter third number=");
    scanf("%d",&c);
    if (a>b && a>c)
    {
        printf("a is greater number");
    }
    else
    if (b>c) 
    {
        printf("b is greater number");
    }
    else 
    {
        printf("c is greater number");
    }
}
